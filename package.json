{
  "name": "minor-watt-now",
  "version": "1.0.0",
  "description": "Monitor for festival power generators",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "watch": "gulp",
    "build-js": "gulp js",
    "build-css": "gulp sass",
    "build": "gulp js && gulp sass",
    "expose": "ngrok http 2500"
  },
  "repository": "lhafkamp/minor-watt-now.git",
  "author": "Luuk Hafkamp, Sjoerd Beentjes, Merlijn Vos",
  "license": "ISC",
  "bugs": "https://github.com/lhafkamp/minor-watt-now/issues",
  "homepage": "https://github.com/lhafkamp/minor-watt-now#readme",
  "dependencies": {
    "csv-parse": "^1.2.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "socket.io": "^1.7.4",
    "socket.io-client": "^1.7.4"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-browserify": "^0.5.1",
    "gulp-concat": "^2.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.1.2",
    "xo": "^0.18.2"
  },
  "xo": {
    "space": true,
    "env": [
      "node",
      "browser"
    ],
    "rules": {
      "operator-linebreak": 0,
      "camelcase": 0,
      "no-unused-expressions": [
        "error",
        {
          "allowTernary": true
        }
      ]
    }
  }
}
